.painter
  margin: 2em
  border: 1px solid $tertiary
  background: $secondary
  position: relative
  
  > canvas
    display: block
    position: relative
    width: 100%
  
  .subject
    background: $tertiary
    
  .classifier
    position: absolute
    top: 0
    left: 0
    background: rgba(255, 128, 255, 0.5)
